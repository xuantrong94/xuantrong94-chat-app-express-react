{
  "env": {
    "browser": false,
    "node": true,
    "es2021": true
  },
  "extends": ["eslint:recommended", "plugin:prettier/recommended"],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": ["error", { "endOfLine": "lf" }],
    "no-console": "warn",
    "semi": ["error", "always"]
  }
}
